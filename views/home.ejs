<%- include("partials/header"); -%>

<h1>Home</h1>

<p><%= startingContent %></p>

<% postList.forEach(function(item){ %>
    <h2><%= item.title %></h2>
    <p class="post-content">
        <%= item.content.substring(0, 100) + " ... " %> 
        <a href="/posts/<%= item._id %>">Read More</a>
    </p>
    
<% }); %>

<%- include("partials/footer"); -%>